{
    "editor.formatOnSave": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "cSpell.words": [
        "bunx",
        "Lodex",
        "loterre",
        "Loterre",
        "NETSCITY",
        "skos",
        "venv",
        "webdav"
    ],
    "files.watcherExclude": {
        "**/.dvc/cache/**": true,
        "**/.dvc/tmp/**": true,
        "**/.venv/**": true,
        "**/databases/**": true,
        "**/node_modules/**": true,
        "**/data/**": true
    },
    "search.exclude": {
        "**/.dvc/cache/**": true,
        "**/.dvc/tmp/**": true,
        "**/.venv/**": true,
        "**/databases/**": true,
        "**/node_modules/**": true,
        "**/data/**": true
    }
}
